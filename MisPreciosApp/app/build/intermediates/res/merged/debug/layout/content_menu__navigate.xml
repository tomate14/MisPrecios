<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.maxi.mispreciosapp.Menu_Navigate"
    tools:showIn="@layout/app_bar_menu__navigate">

    <TextView
        android:id="@+id/textBienvenida"
        android:layout_width="375dp"
        android:layout_height="357dp"
        android:fontFamily="Time New Roman"
        android:textStyle="bold"
        android:text=""
        android:textSize="20dp"
        tools:layout_editor_absoluteY="10dp"
        tools:layout_editor_absoluteX="10dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnScan"
        android:layout_width="128dp"
        android:layout_height="55dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="50dp"
        android:background="@color/colorPrimary"
        android:text="Scanear"
        android:textColor="@color/colorAccent"
        android:textSize="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textBienvenida"
        app:layout_constraintVertical_bias="0.184"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="417dp" />


</android.support.constraint.ConstraintLayout>
